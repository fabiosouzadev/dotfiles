          "xsel",
          "zip",
          "wine",
          "zoxide"
        ],
        "ppas": [
          {
            "name": "ubuntu-mozilla-daily/ppa",
            "packages": ["firefox-trunk"]
          }
        ],
        "sources": [
          {
            "name": "antigravity",
            "keyring_path": "/etc/apt/keyrings/antigravity.gpg",
            "key_url": "https://us-central1-apt.pkg.dev/doc/repo-signing-key.gpg",
            "repo": "deb [signed-by=/etc/apt/keyrings/antigravity.gpg] https://us-central1-apt.pkg.dev/projects/antigravity-auto-updater-dev/ antigravity-debian main",
            "packages": ["antigravity"]
          },
          {
            "name": "dbeaver",
            "keyring_path": "/etc/apt/keyrings/dbeaver.gpg",
            "key_url": "https://dbeaver.io/debs/dbeaver.gpg.key",
            "repo": "deb [signed-by=/etc/apt/keyrings/dbeaver.gpg] https://dbeaver.io/debs/dbeaver-ce /",
            "packages": ["dbeaver-ce"]
          },
          {
            "name": "firefox",
            "keyring_path": "/etc/apt/keyrings/mozilla-linux.gpg",
            "key_url": "https://packages.mozilla.org/apt/repo-signing-key.gpg",
            "repo": "deb [signed-by=/etc/apt/keyrings/mozilla-linux.gpg] https://packages.mozilla.org/apt mozilla main",
            "packages": ["firefox-devedition", "firefox-beta"]
          },
                    {
            "name": "google-chrome",
            "key_url": "https://dl-ssl.google.com/linux/linux_signing_key.pub",
            "keyring_path": "/etc/apt/keyrings/google-linux.gpg",
            "repo": "deb [arch=amd64 signed-by=/etc/apt/keyrings/google-linux.gpg] http://dl.google.com/linux/chrome/deb/ stable main",
            "packages": ["google-chrome-unstable"]
          },
          {
            "name": "vscode",
            "keyring_path": "/etc/apt/keyrings/microsoft.gpg",
            "key_url": "https://packages.microsoft.com/keys/microsoft.asc",
            "repo": "deb [arch=amd64 signed-by=/etc/apt/keyrings/microsoft.gpg] https://packages.microsoft.com/repos/code stable main",
            "packages": ["code-insiders"]
          },
          {
            "name": "spotify",
            "keyring_path": "/etc/apt/keyrings/spotify.gpg",
            "key_url": "https://download.spotify.com/debian/pubkey_5384CE82BA52C83A.asc",
            "repo": "deb [signed-by=/etc/apt/keyrings/spotify.gpg] https://repository.spotify.com stable non-free",
            "packages": ["spotify-client"]
          }
        ],
        "debs": [
          {
            "name": "fd",
            "url": "https://github.com/sharkdp/fd/releases/download/v10.3.0/fd_10.3.0_amd64.deb"

          },
          {
