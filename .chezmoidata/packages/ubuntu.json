          },
          {
            "name": "vesktop",
            "url": "https://vencord.dev/download/vesktop/amd64/deb"
          },
          {
            "name": "yazi",
            "url": "https://github.com/sxyazi/yazi/releases/download/nightly/yazi-x86_64-unknown-linux-gnu.deb"
          }
        ]
      }
    }
  }
}