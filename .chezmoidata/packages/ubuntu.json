{
  "packages": {
    "linux": {
      "ubuntu": {
        "apt": [
          "bat",
          "clang",
          "dex",
          "direnv",
          "dunst",
          "eza",
          "feh",
          "flameshot",
          "git",
          "gnome-keyring",
          "gzip",
          "htop",
          "i3",
          "jq",
          "kitty",
          "make",
          "mpv",
